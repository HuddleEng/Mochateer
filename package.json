{
    "name": "muppeteer",
    "version": "1.3.0",
    "description": "Visual regression testing framework for running UI tests in Chrome",
    "main": "lib/index.js",
    "scripts": {
        "test": "npm run unit; npm run e2e",
        "unit": "jest --testPathPattern=\"^((?!e2e|examples).)*$\" --coverage --collectCoverageFrom='(src|lib)/**.{js,jsx}' && cat ./coverage/lcov.info | codacy-coverage",
        "e2e": "node e2e/index.js",
        "e2e-debug": "node --trace-warnings e2e/index.js",
        "example-unit-tests": "node examples/runTests.js -u",
        "example-e2e-tests": "node examples/runTests.js -e",
        "postinstall": "node examples/setup.js",
        "lint": "eslint **/*.js --fix --ignore-pattern node_modules",
        "prettier": "prettier --write --single-quote --tab-width=4 */**.js",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.js": [
            "prettier --write --single-quote --tab-width=4",
            "git add"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HuddleEng/Muppeteer.git"
    },
    "author": "Gideon Pyzer / Huddle",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/HuddleEng/Muppeteer/issues"
    },
    "homepage": "https://github.com/HuddleEng/Muppeteer#readme",
    "bin": {
        "muppeteer": "bin/launcherCli.js"
      },
    "dependencies": {
        "chai": "^4.1.2",
        "codacy-coverage": "^3.0.0",
        "colors": "^1.1.2",
        "commander": "^2.15.1",
        "extract-zip": "^1.6.6",
        "glob": "^7.1.2",
        "mkdirp": "^0.5.1",
        "mocha": "^5.0.0",
        "mocha-junit-reporter": "^1.17.0",
        "mocha-multi-reporters": "^1.1.7",
        "mochawesome": "^3.0.2",
        "pixelmatch": "^4.0.2",
        "pngjs": "^3.3.2",
        "puppeteer": "^1.10.0",
        "puppeteer-extensions": "^1.0.0",
        "recursive-readdir-sync": "^1.0.6",
        "request": "^2.83.0",
        "request-promise-native": "^1.0.5",
        "sanitize-filename": "^1.6.1",
        "shelljs": "^0.8.1",
        "sync-request": "^6.0.0",
        "yargs": "^12.0.5"
    },
    "devDependencies": {
        "eslint": "^5.9.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.0",
        "express": "^4.16.2",
        "find-node-modules": "^1.0.4",
        "husky": "^1.2.0",
        "jest": "^23.6.0",
        "lint-staged": "^8.1.0",
        "minimist": "^1.2.0",
        "prettier": "^1.11.1"
    }
}
